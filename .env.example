# 3DS API Configuration
# Copy this file to .env and fill in your values

# Your 3DS API key (sandbox key for testing)
API_KEY=your-api-key-here

# API base URL
API_BASE_URL=https://service.sandbox.3dsecure.io

# Route through local application stack instead of direct 3DS calls
# Set to 'true' to use local proxy at http://fbf-cde-card-linking-web-sdk-cde.localhost
USE_LOCAL_PROXY=false

# Local proxy base URL (used when USE_LOCAL_PROXY=true)
LOCAL_PROXY_URL=http://fbf-cde-card-linking-web-sdk-cde.localhost

# Server port (optional, defaults to 3002)
PORT=3002